{
    "prettier.enable": false,
    "editor.formatOnSave": false,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "editor.quickSuggestions": {
        "strings": true
    },
    "cSpell.words": [
        "ofetch",
        "Twooth",
        "Unsubscriber",
        "Writables"
    ],
    "dotenv.enableAutocloaking": false,
}
